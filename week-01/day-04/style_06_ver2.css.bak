body {
	padding: 50px;
	font-family: Consolas;
}

div {
	width: 136px;
	height: 136px;
	line-height: 136px;
	text-align: center;	
	background: #B90CEB;
	-moz-border-radius: 68px;
	-webkit-border-radius: 68px;
	border-radius: 68px;
	display: inline-block;
	margin: 5px;
	color: #fff;
	font-size: 32px;
}

div:nth-child(n+2) {
	background: #0CEBDE;
}
div:last-child {
	background: #EBBE0C;
}